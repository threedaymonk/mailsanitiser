{
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["http://www.dailymail.co.uk/*"],
    "css": ["sanitise.css"],
    "js": ["sanitise.js"]
  }],
  "description": "Removes everything except the article text from Daily Mail pages",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "name": "Mail Sanitiser",
  "version": "1.1.0"
}
